{
  "name": "twin-ui",
  "version": "1.3.4",
  "description": "component library for react",
  "license": "MIT",
  "author": "patrickap",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickap/twin-ui.git"
  },
  "type": "module",
  "files": [
    "./dist"
  ],
  "main": "./dist/twin-ui.umd.cjs",
  "module": "./dist/twin-ui.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "styles": "./dist/index.css",
      "import": "./dist/twin-ui.js",
      "require": "./dist/twin-ui.umd.cjs"
    }
  },
  "types": "./dist/index",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsc && vite build",
    "release": "npm run build && sh release.sh"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.15",
    "clsx": "^2.1.1",
    "motion": "^12.6.5",
    "radix-ui": "^1.2.0",
    "react-responsive": "^10.0.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/vite": "^4.1.3",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-static-copy": "^2.3.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0"
  },
  "keywords": [
    "component",
    "layout",
    "library",
    "react",
    "typescript",
    "accessible",
    "interface",
    "ui"
  ]
}
