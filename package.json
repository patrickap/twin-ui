{
  "name": "twin-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "dist/twin-ui.cjs.js",
  "module": "dist/twin-ui.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/dist/index.css"
  ],
  "scripts": {
    "storybook:watch": "concurrently 'start-storybook -p 1234' 'npm run storybook:watch:css'",
    "storybook:watch:css": "tailwindcss -w -m -i ./src/index.css -o ./src/index.min.css",
    "storybook:build": "build-storybook && npm run storybook:build:css",
    "storybook:build:css": "tailwindcss -m -i ./src/index.css -o ./src/index.min.css",
    "package:build": "tsc && vite build && npm run package:build:css",
    "package:build:css": "tailwindcss -m -i ./src/index.css -o ./dist/index.css"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.15",
    "@radix-ui/react-alert-dialog": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.2",
    "@radix-ui/react-dropdown-menu": "^2.0.3",
    "@radix-ui/react-toast": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.0.4",
    "clsx": "^1.2.1",
    "framer-motion": "^10.0.1",
    "pullstate": "^1.25.0",
    "react-responsive": "^9.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^0.6.11",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.1.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
