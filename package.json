  "description": "component library for react",
  "license": "MIT",
  "author": "patrickap",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickap/twin-ui.git"
  },
  "type": "module",
  "files": [
    "./dist"
  ],
  "main": "./dist/twin-ui.umd.cjs",
