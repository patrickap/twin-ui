{
  "name": "twin-ui",
  "version": "1.7.0",
  "description": "component library for react",
  "license": "MIT",
  "author": "patrickap",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickap/twin-ui.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "style": "./dist/index.css",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "style": "./dist/index.css",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsc && vite build",
    "release": "npm run build && sh release.sh"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "clsx": "^2.1.1",
    "motion": "^12.23.24",
    "radix-ui": "^1.4.3",
    "react-responsive": "^10.0.1",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@storybook/react-vite": "^10.0.8",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "storybook": "^10.0.8",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0"
  },
  "keywords": [
    "component",
    "layout",
    "library",
    "react",
    "typescript",
    "accessible",
    "interface",
    "ui"
  ]
}
