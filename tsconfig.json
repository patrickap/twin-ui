{
	"compilerOptions": {
    "jsx": "react-jsx",
		"target": "ES6",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"types": ["node"],
		"strict": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"include": ["./src"]
}
